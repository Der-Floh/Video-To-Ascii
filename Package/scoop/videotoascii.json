{
    "version": "{{VERSION}}",
    "description": "A .NET console application that converts and plays videos as colourful ASCII art in the terminal.",
    "homepage": "https://github.com/Der-Floh/Video-To-Ascii",
    "license": "MIT",
    "architecture": {
        "64bit": {
            "url": "https://github.com/Der-Floh/Video-To-Ascii/releases/download/v{{VERSION}}/VideoToAscii-{{VERSION}}.zip",
            "hash": "{{SHA256}}"
        },
        "32bit": {
            "url": "https://github.com/Der-Floh/Video-To-Ascii/releases/download/v{{VERSION}}/VideoToAscii-{{VERSION}}.zip",
            "hash": "{{SHA256}}"
        }
    },
    "bin": "VideoToAscii.exe",
    "depends": [
        "versions/dotnet-runtime-9"
    ],
    "checkver": {
        "github": "https://github.com/Der-Floh/Video-To-Ascii",
        "regex": "v([\\d.]+)"
    },
    "autoupdate": {
        "url": "https://github.com/Der-Floh/Video-To-Ascii/releases/download/v$version/VideoToAscii-$version.zip",
        "hash": {
            "url": "$url.sha256"
        }
    }
}